<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/verde_claro"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="426dp"
        android:layout_height="352dp"
        app:layout_constraintBottom_toTopOf="@+id/mesita"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logo_cafe_tierno_1_" />

    <ImageView
        android:id="@+id/mesita"
        android:layout_width="408dp"
        android:layout_height="169dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mesita_removebg_preview"
        tools:layout_editor_absoluteY="352dp" />

    <ImageView
        android:id="@+id/gato"
        android:layout_width="132dp"
        android:layout_height="184dp"
        android:layout_marginTop="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/flores1"
        app:layout_constraintHorizontal_bias="0.322"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mesita"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/gato_removebg_preview" />

    <ImageView
        android:id="@+id/flores1"
        android:layout_width="88dp"
        android:layout_height="138dp"

        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mesita"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/flores1_removebg_preview" />

    <ImageView
        android:id="@+id/flores2"
        android:layout_width="73dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/gato"
        app:layout_constraintTop_toBottomOf="@+id/mesita"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/flores1_removebg_preview" />


</androidx.constraintlayout.widget.ConstraintLayout>
